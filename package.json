{
  "name": "shop-wxapp",
  "version": "0.0.0",
  "description": "shop-wxapp",
  "scripts": {
    "start": "cross-env NODE_ENV=production webpack --watch --progress",
    "dev": "cross-env NODE_ENV=dev webpack --watch --progress -p",
    "webpack": "webpack",
    "prestart": "rimraf dist",
    "prebuild": "rimraf dist",
    "build:wechat": "cross-env NODE_ENV=production webpack --env.target=Wechat -p --progress",
    "build": "run-s build:*",
    "c-p": "create-wxapp-page --dir=src --indent=tab --style=scss",
    "c-c": "create-wxapp-page --type=component --dir=src --indent=tab --style=scss",
    "r": "rimraf ./node_modules",
    "fix": "eslint --fix *.js ./src"
  },
  "main": "dist/wechat/app.js",
  "author": "jc",
  "license": "MIT",
  "engines": {
    "node": ">=4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "copy-webpack-plugin": "^4.2.0",
    "create-wxapp-page": "^2.0.6",
    "cross-env": "^5.0.1",
    "eslint": "^4.16.0",
    "eslint-config-alloy": "^1.4.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-node": "^5.2.1",
    "file-loader": "1.1.11",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.4.1",
    "wxapp-webpack-plugin": "^0.16.0",
    "wxml-loader": "^0.2.1",
    "uglifyjs-webpack-plugin": "^1.3.0"
  },
  "dependencies": {
    "decimal.js": "^10.0.1",
    "es6-promise": "^4.1.0",
    "peanut-all": "^1.1.1"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules",
      "lib"
    ]
  }
}
